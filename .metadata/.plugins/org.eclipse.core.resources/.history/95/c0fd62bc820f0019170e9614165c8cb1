using std::vector;

int main() {
	std::string str;
	std::vector<char> stack;
	std::cin >> str;

	for (unsigned int i = 0; i < str.length(); i++) {
		stack.push_back(str[i]);
	}

	std::
	cout << stack[0] << std::endl;
	//std::cout << str << std::endl;

	return 0;
}




