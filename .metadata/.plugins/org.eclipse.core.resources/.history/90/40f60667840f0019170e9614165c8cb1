#include <iostream>
#include <stdio.h>
#include <string>
#include <vector>
using std::string;
using std::vector;

bool types_equal(char a, char b) {
	return true;
}

int main() {
	std::string str;
	std::vector<char> stack;
	std::cin >> str;

	for (unsigned int i = 0; i < str.length(); i++) {
		if ((str[i] = '(')||(str[i] = '[')||(str[i] == '{')) {
			stack.push_back(str[i]);
		} else {
			if ((types_equal(str[i], stack[stack.size() - 1]))&&(stack.size() > 0)) {
				stack.pop_back();
			} else {
				std::cout << i << std::endl;
				exit(0);
			}
		}
	}

	if (stack.size() > 0) {
		std::cout << str.length() << std::endl;
	} else  {
		std::cout << "correct" << std::endl;
	}

	//std::cout << str << std::endl;
	return 0;
}




